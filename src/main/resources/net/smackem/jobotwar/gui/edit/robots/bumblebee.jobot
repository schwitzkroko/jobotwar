// robot bumblebee

def destX, destY

newloc:
    50 + RANDOM * 540 -> destX
    50 + RANDOM * 380 -> destY
    destX - X -> SPEEDX
    destY - Y -> SPEEDY

move:
    0 -> SPEEDX if abs(destX - X) < 30
    0 -> SPEEDY if abs(destY - Y) < 30
    goto move unless SPEEDX = 0 and SPEEDY = 0
    goto newloc
